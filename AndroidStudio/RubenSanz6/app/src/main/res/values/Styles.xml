    <style name="styleBoton">
        <item name="android:textSize">25dp</item>
        <item name="android:fontFamily">@font/architects_daughter</item>
        <item name="android:backgroundTint">@android:color/holo_purple</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:shadowColor">@android:color/darker_gray</item>
        <item name="cornerRadius">25dp</item>
    </style>
